        "getCollection": {
            "method": "GET",
            "url": {
                "en": "\/products"
            },
            "description": {
                "en": "Used to retrieve a collection of elements."
            },
            "parameters": [
                {
                    "name": "page",
                    "description": {
                        "en": "The serial number of the given page."
                    }
                },
                {
                    "name": "limit",
                    "description": {
                        "en": "Number of items displayed on a page. Maximum 200."
                    }
                },
                {
                    "name": "full",
                    "description": {
                        "en": "Whether the values of the first-level fields should be expanded. Value: 1 (yes) or 0 (no). Default: 0."
                    }
                },
                {
                    "name": "innerId",
                    "description": {
                        "en": "Search parameter. Product internal identifier."
                    }
                },
                {
                    "name": "sku",
                    "description": {
                        "en": "Search parameter. Product sku."
                    }
                },
                {
                    "name": "createdAt",
                    "description": {
                        "en": "Search parameter. It can be used for search the creation date. Returns products created on the specified date. Format: 2013-01-01."
                    }
                },
                {
                    "name": "updatedAt",
                    "description": {
                        "en": "Search parameter. It can be used for search the modification date. Returns products modified on the specified date. Format: 2013-01-01."
                    }
                },
                {
                    "name": "parentProductId",
                    "description": {
                        "en": "Search parameter. According to the parent product innerId, it will filter to product having the same parent."
                    }
                }
            ],
            "example": {
                "url": "\/products?full=1&page=0&limit=5",
                "statusCode": "200"
            }
        },
